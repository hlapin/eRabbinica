               <persName type="lemma">שלמן<ref corresp="#d2e46690"/>
               </persName>
               <persName type="lemma" xml:lang="en">Shalman</persName>
               <persName xml:lang="checkme">סלימן<ref corresp="#d1187e3"/>
               </persName>
               <persName xml:lang="checkme"/>
               <state type="desc">
                  <note>Moses (8)’s son</note>
               </state>
               <state type="provenance">
                  <note>Inscription, Temple Mount, Jerusalem</note>
               </state>
               <bibl>Sukenik, “Jewish Inscriptions,” 139</bibl>
               <state type="addl-Descr">
                  <note>Late?<ref corresp="#d2e46816"/>Second name</note>
