               <sex cert="high">M</sex>
               <persName type="lemma">יצחק</persName>
               <persName type="lemma" xml:lang="en">Isaac</persName><ref type="note" corresp="#d875e4"/>
               <state type="desc">
                  <note>–</note>
               </state>
               <state type="provenance">
                  <note>Epitaph, Hebron?<ref type="note" corresp="#d2e34286"/>
                  </note>
               </state>
               <bibl>Mordtmann,ZDPV12 (1889) 132</bibl>
               <state type="addl-Descr">
                  <note>Jew?<ref type="note" corresp="#d2e34300"/>
                  </note>
