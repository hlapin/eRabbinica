               <persName type="lemma">אברהם<ref corresp="#d2e343"/>
               </persName>
               <persName type="lemma" xml:lang="en">Abraham</persName>
               <persName xml:lang="checkme">–</persName>
               <state type="desc">
                  <note>Or (1)’s son, Shemta‘ima<ref corresp="#d2e451"/>leader<ref corresp="#d2e455"/>
                  </note>
               </state>
               <state type="provenance">
                  <note>–</note>
