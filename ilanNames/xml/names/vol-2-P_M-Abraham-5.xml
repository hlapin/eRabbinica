               <sex cert="high">M</sex>
               <persName type="lemma">אברהם<ref corresp="#d2e343"/>
               </persName>
               <persName type="lemma" xml:lang="en">Abraham</persName>
               <persName xml:lang="checkme">Abraha<ref corresp="#d12e3"/>
               </persName>
               <state type="desc">
                  <note>The Syrian?<ref corresp="#d2e527"/>
                  </note>
               </state>
               <state type="provenance">
                  <note>Epitaph, Celeia,<ref corresp="#d2e534"/>Diehl</note>
               </state>
               <bibl>CIJ91*</bibl>
               <state type="addl-Descr">
                  <note>Christian?<ref corresp="#d2e547"/>
                  </note>
