               <listPerson xml:id="Abl-Vol-2-Vol-4-ADD-A_F" n="ابعلي">
                  <person xml:id="Vol-2-Vol-4-ADD-A_F-d8e16-person"
                          n="Vol-2-Vol-4-ADD-A_F-Arabic-Females-Abī‘lī-1">
                     <sex cert="high">F</sex>
                     <persName type="lemma">ابعلي</persName>
                     <persName type="lemma" xml:lang="en">Abī‘lī</persName>
                     <persName>אבעלי<ref type="note" corresp="#Vol-2-Vol-4-ADD-A_F-ftn2"/>
                     </persName>
                     <state type="desc">
