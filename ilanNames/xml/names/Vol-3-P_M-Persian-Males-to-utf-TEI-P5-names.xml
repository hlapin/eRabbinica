                     <note xml:id="Vol-3-P_M-ftn51" n="10"> See above, n. 7.</note>
                     <note xml:id="Vol-3-P_M-ftn52" n="11"> The papyrus is dated.</note>
                  </person>
               </listPerson>
            </ab>
            <ab type="name" n="Hyrcanus-Hyrcanus">
               <ref type="note" corresp="#Vol-3-P_M-ftn53"/>
               <listPerson xml:id="Hyrcanus-Vol-3-P_M" n="Hyrcanus">
                  <person xml:id="Vol-3-P_M-d8e724-person" n="Vol-3-P_M-Persian-Males-Hyrcanus-1">
                     <sex cert="high">M</sex>
                     <persName type="lemma">Hyrcanus</persName>
                     <persName type="lemma" xml:lang="en">Hyrcanus</persName>
                     <persName>̔Υρκανός<ref type="note" corresp="#Vol-3-P_M-ftn54"/>
                     </persName>
                     <state type="desc">
