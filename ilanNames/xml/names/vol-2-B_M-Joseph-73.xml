               <persName type="lemma" xml:lang="en">Joseph</persName><ref type="note" corresp="#d670e4"/>
               <state type="desc">
                  <note>Abbin (28)’s son, of Tasele<ref type="note" corresp="#d2e26433"/>
                  </note>
