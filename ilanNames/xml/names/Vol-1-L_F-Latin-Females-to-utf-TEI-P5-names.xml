                  </person>
                  <person xml:id="Vol-1-L_F-d2e538-person" n="Vol-1-L_F-Julia-2">
                     <sex cert="high">F</sex>
                     <persName xml:lang="gr">Ιουλια<ref type="note" corresp="#Vol-1-d456e0"/>
                     </persName>
                     <state type="desc">
                        <note>Of Asia<ref type="note" corresp="#Vol-1-d2e549"/>
