                  <person xml:id="Vol-1-L_F-d2e755-person" n="Vol-1-L_F-Popellia-1">
                     <sex cert="high">F</sex>
                     <persName xml:lang="gr">Ποπέλι<ref type="note" corresp="#Vol-1-d635e0"/>
