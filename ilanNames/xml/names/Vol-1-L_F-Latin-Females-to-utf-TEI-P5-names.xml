                     <persName xml:lang="gr">Σαλίνα<ref type="note" corresp="#Vol-1-d670e0"/>
                     </persName>
                     <state type="desc">
                        <note>Alexandra (3), Aristobulus (1)’s wife</note>
                     </state>
                     <state type="provenance">
                        <note>—</note>
                     </state>
                     <bibl>AJ13:320 (Schalit,NB, -)</bibl>
                     <state type="addl-Descr">
                        <note>—</note>
                     </state>
