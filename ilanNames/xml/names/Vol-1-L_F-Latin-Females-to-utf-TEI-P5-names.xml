            <ab type="name" n="Popellia-Popellia">
               <ref type="note" corresp="#Vol-1-L_F-ftn54"/>
               <listPerson xml:id="Popellia-Vol-1-L_F" n="Popellia">
                  <person xml:id="Vol-1-L_F-d2e755-person" n="Vol-1-L_F-Popellia-1">
                     <persName type="lemma">Popellia</persName>
                     <persName type="lemma" xml:lang="en">Popellia</persName>
                     <sex cert="high">F</sex>
                     <persName xml:lang="gr">Ποπέλι<ref type="note" corresp="#Vol-1-L_F-ftn55"/>
                     </persName>
                     <state type="desc">
