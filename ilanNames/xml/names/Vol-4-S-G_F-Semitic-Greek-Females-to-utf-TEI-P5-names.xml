               <ref type="note" corresp="#Vol-4-S-G_F-ftn1"/>
               <listPerson xml:id="Amathbel-Vol-4-S-G_F" n="Amathbel">
                  <person xml:id="Vol-4-S-G_F-d8e16-person"
                          n="Vol-4-S-G_F-Semitic-Greek-Females-Amathbel-1">
                     <sex cert="high">F</sex>
                     <persName type="lemma">Amathbel</persName>
                     <persName type="lemma" xml:lang="en">Amathbel</persName>
                     <persName>[̓́Α]μαθβηλ<ref type="note" corresp="#Vol-4-S-G_F-ftn2"/>
                     </persName>
                     <state type="desc">
                        <note>–</note>
                     </state>
                     <state type="provenance">
                        <note>Graffito, synagogue Dura-Europos, du Mesnil</note>
                     </state>
                     <bibl>IJO3, Syr90</bibl>
                     <state type="addl-Descr">
                        <note>Jew?<ref type="note" corresp="#Vol-4-S-G_F-ftn3"/>P:Dura-Europos</note>
