               <persName type="lemma">נריה<ref corresp="#d2e41382"/>
               </persName>
               <persName type="lemma" xml:lang="en">Neriyah</persName>
               <persName xml:lang="gr"><ref corresp="#d1057e4"/>
               </persName>
