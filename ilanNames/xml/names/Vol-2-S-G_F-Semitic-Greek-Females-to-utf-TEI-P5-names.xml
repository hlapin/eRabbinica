                  <person xml:id="Vol-2-S-G_F-d8e529-person"
                          n="Vol-2-S-G_F-Semitic-Greek-Females-Φελφέλα Felfela-1">
                     <sex cert="high">F</sex>
                     <persName type="lemma">Φελφέλα Felfela</persName>
                     <persName type="lemma" xml:lang="en">Φελφέλα Felfela</persName>
                     <persName>–</persName>
                     <state type="desc">
                        <note>Marcelina (1)</note>
                     </state>
                     <state type="provenance">
                        <note>Amulet, Samaria</note>
                     </state>
                     <bibl>Piccirillo,Historiam pictura, 468</bibl>
                     <state type="addl-Descr">
                        <note>Samaritan?<ref type="note" corresp="#Vol-2-S-G_F-ftn40"/>Second name</note>
                     </state>
                     <floruit>572<ref type="note" corresp="#Vol-2-S-G_F-ftn41"/>
                     </floruit>
                     <note xml:id="Vol-2-S-G_F-ftn40" n="2"> The amulet comes from the region of Samaria and displays no Christian signs, but see Introduction 6.1.5.</note>
                     <note xml:id="Vol-2-S-G_F-ftn41" n="3"> The amulet is dated according to the Byzantine indictions, see Introduction 7.5.4.2.</note>
                  </person>
               </listPerson>
            </ab>
            <ab type="name" n="Φοσευάθη Foseuati-Φοσευάθη Foseuati">
               <ref type="note" corresp="#Vol-2-S-G_F-ftn42"/>
               <listPerson xml:id="_Foseuati-Vol-2-S-G_F" n="Φοσευάθη Foseuati">
                  <person xml:id="Vol-2-S-G_F-d8e574-person"
                          n="Vol-2-S-G_F-Semitic-Greek-Females-Φοσευάθη Foseuati-1">
                     <sex cert="high">F</sex>
                     <persName type="lemma">Φοσευάθη Foseuati</persName>
                     <persName type="lemma" xml:lang="en">Φοσευάθη Foseuati</persName>
                     <persName>–</persName>
                     <state type="desc">
                        <note>Parthenius (1)’s daughter</note>
                     </state>
                     <state type="provenance">
                        <note>Epitaph, Zoar</note>
                     </state>
                     <bibl>IPT1a, no. 301</bibl>
                     <state type="addl-Descr">
                        <note>Jew?<ref type="note" corresp="#Vol-2-S-G_F-ftn43"/>
                        </note>
                     </state>
                     <floruit>4th-6thC<ref type="note" corresp="#Vol-2-S-G_F-ftn44"/>
