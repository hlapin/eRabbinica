            <optional>
               <ref name="tei_placeName"/>
            </optional>
            <optional>
               <ref name="tei_country"/>
            </optional>
            <optional>
               <ref name="tei_region"/>
            </optional>
            <optional>
               <ref name="tei_settlement"/>
            </optional>
            <optional>
               <ref name="tei_district"/>
            </optional>
     
            <optional>
               <ref name="tei_institution"/>
            </optional>
     
     
            <optional>
               <ref name="tei_repository"/>
            </optional>
     
     
            <optional>
               <ref name="tei_collection"/>
            </optional>
     
            <ref name="tei_idno"/>
     
            <optional>
               <ref name="tei_note"/>
            </optional>
     
         </group>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_msName">
      <element name="msName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(alternative name) contains any form of unstructured alternative name used for a manuscript or other object, such as an ocellus nominum, or nickname. [10.4. The Manuscript Identifier]</a:documentation>
