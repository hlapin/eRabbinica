            <ab corresp="#r_YerushalmiVeniceEp_Page_479-eSc_textblock_bb83cd52"
                type="Paratext">
               <l corresp="#l_YerushalmiVeniceEp_Page_479-eSc_line_d12f1602">היו בודקין פרק שנים עשר</l>
            </ab>
