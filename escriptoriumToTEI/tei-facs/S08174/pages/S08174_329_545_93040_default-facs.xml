            <ab corresp="#r_545_93040_default-eSc_textblock_b90bfa2e" type="Main">
               <l corresp="#l_545_93040_default-eSc_line_1939628c">ה'</l>
               <l corresp="#l_545_93040_default-eSc_line_53243c35">
                  <milestone unit="ab" n="6.11.4.5"/>תרגום  שב  </l>
               <l corresp="#l_545_93040_default-eSc_line_821eb32e">עזרה ושבדניאל מטמא את הידים</l>
               <l corresp="#l_545_93040_default-eSc_line_3dff76b9">תרגום שכתבו עברית ועברית שכתבו</l>
               <l corresp="#l_545_93040_default-eSc_line_2d3aa3c6">תרגום כתב עברי אינו מטמא עד</l>
               <l corresp="#l_545_93040_default-eSc_line_346d5845">שיכתבנו אשורית על העור כדיו:</l>
            </ab>
            <ab corresp="#r_545_93040_default-eSc_textblock_66d19eea" type="Commentary">
               <l corresp="#l_545_93040_default-eSc_line_8ba586fa"/>
