            <ab corresp="#r_97_fe7f2_default-eSc_textblock_c97c19e7" type="Paratext">
               <l corresp="#l_97_fe7f2_default-eSc_line_c03f88bb">מככת חולין</l>
            </ab>
            <ab corresp="#r_97_fe7f2_default-eSc_textblock_202afc90" type="Commentary">
               <l corresp="#l_97_fe7f2_default-eSc_line_74606329"/>
