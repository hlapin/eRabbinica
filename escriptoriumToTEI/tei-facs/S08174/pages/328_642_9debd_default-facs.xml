            <ab corresp="#f_642_9debd_default-eSc_dummyblock_" type="-fw">
               <l corresp="#f_642_9debd_default-eSc_line_a52e9a3d">יהא</l>
               <l corresp="#f_642_9debd_default-eSc_line_838b0a0e">אלאק</l>
               <l corresp="#f_642_9debd_default-eSc_line_0676c83e">שיבןג</l>
               <l corresp="#f_642_9debd_default-eSc_line_0167674d">ול האבאא</l>
               <l corresp="#f_642_9debd_default-eSc_line_6a020a33">ש' חיאנ'</l>
               <l corresp="#f_642_9debd_default-eSc_line_229c0240">ש'</l>
               <l corresp="#f_642_9debd_default-eSc_line_e19f30b0">ולנוש:</l>
               <l corresp="#f_642_9debd_default-eSc_line_01e3774d">וור</l>
               <l corresp="#f_642_9debd_default-eSc_line_454b0775">וג'</l>
               <l corresp="#f_642_9debd_default-eSc_line_01d4f789"/>
               <l corresp="#f_642_9debd_default-eSc_line_7b127d1c">ואב' אואם נבונשקן</l>
               <l corresp="#f_642_9debd_default-eSc_line_750a5189">כח 1ל</l>
               <l corresp="#f_642_9debd_default-eSc_line_1d6f94b6">גס וזן'</l>
               <l corresp="#f_642_9debd_default-eSc_line_7bc6de96">דיו יזז</l>
            </ab>
