   <text>
      <body>
         <div>
            <pb xml:id="p_329_073_1da75_default"/>
            <cb xml:id="c_329_073_1da75_default-A"/>
            <ab corresp="#r_73_1da75_default-eSc_textblock_f0df9aa1" type="Paratext">
               <l corresp="#l_73_1da75_default-eSc_line_247d4cc0">יסכת מנחות</l>
            </ab>
            <ab corresp="#r_73_1da75_default-eSc_textblock_ef5f6dca" type="Commentary">
               <l corresp="#l_73_1da75_default-eSc_line_3996a7fe"/>
               <l corresp="#l_73_1da75_default-eSc_line_6835051b"/>
               <l corresp="#l_73_1da75_default-eSc_line_93d6b568"/>
               <l corresp="#l_73_1da75_default-eSc_line_ffa4f800"/>
