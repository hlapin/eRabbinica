            <ab corresp="#r_473_c4349_default-eSc_textblock_b74e87c5" type="Paratext">
               <l corresp="#l_473_c4349_default-eSc_line_ffcee749">מסכ' מקואות</l>
            </ab>
            <ab corresp="#r_473_c4349_default-eSc_textblock_5719133d" type="Commentary">
               <l corresp="#l_473_c4349_default-eSc_line_5cc31a25"/>
               <l corresp="#l_473_c4349_default-eSc_line_227674c5"/>
