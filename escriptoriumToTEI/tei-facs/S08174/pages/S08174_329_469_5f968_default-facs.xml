            <ab corresp="#r_469_5f968_default-eSc_textblock_af544c51" type="Paratext">
               <l corresp="#l_469_5f968_default-eSc_line_77e6c69a">מסכ' מחוואות</l>
            </ab>
            <ab corresp="#r_469_5f968_default-eSc_textblock_4fe000ac" type="Commentary">
               <l corresp="#l_469_5f968_default-eSc_line_f2a885f7"/>
