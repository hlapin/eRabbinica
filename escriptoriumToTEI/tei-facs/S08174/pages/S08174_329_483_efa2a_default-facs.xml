      <body>
         <div>
            <pb xml:id="p_329_483_efa2a_default"/>
            <cb xml:id="c_329_483_efa2a_default-A"/>
            <ab corresp="#r_483_efa2a_default-eSc_textblock_3b6a1844" type="Paratext">
               <l corresp="#l_483_efa2a_default-eSc_line_492b19af">מסכת נידה</l>
            </ab>
            <ab corresp="#r_483_efa2a_default-eSc_textblock_dab63dbf" type="Commentary">
               <l corresp="#l_483_efa2a_default-eSc_line_204ecd45"/>
