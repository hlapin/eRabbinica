            <ab corresp="#f_168_0ba17_default-eSc_textblock_69d1df8d" type="Paratext">
               <l corresp="#f_168_0ba17_default-eSc_line_fdb2781a">לפי</l>
               <l corresp="#f_168_0ba17_default-eSc_line_becde27f">ח'</l>
               <l corresp="#f_168_0ba17_default-eSc_line_1531c921"/>
            </ab>
