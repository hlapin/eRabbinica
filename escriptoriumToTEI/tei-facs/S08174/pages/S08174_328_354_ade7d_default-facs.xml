      <body>
         <div>
            <pb xml:id="p_328_354_ade7d_default"/>
            <cb xml:id="c_328_354_ade7d_default-A"/>
            <ab corresp="#r_354_ade7d_default-eSc_textblock_1f30da88" type="Paratext">
               <l corresp="#l_354_ade7d_default-eSc_line_c7f3fa69">ר</l>
            </ab>
            <ab corresp="#r_354_ade7d_default-eSc_textblock_dd3ce988" type="Commentary">
               <l corresp="#l_354_ade7d_default-eSc_line_fd5ded8f"/>
               <l corresp="#l_354_ade7d_default-eSc_line_76f4567f"/>
