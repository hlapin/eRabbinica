            <ab corresp="#r_401_c3ec8_default-eSc_textblock_0f719f42" type="Paratext">
               <l corresp="#l_401_c3ec8_default-eSc_line_cd0fd2a6">מסכת נגעים</l>
            </ab>
            <ab corresp="#r_401_c3ec8_default-eSc_textblock_3aee41c1" type="Commentary">
               <l corresp="#l_401_c3ec8_default-eSc_line_0a2b4228"/>
