            <ab corresp="#f_74_5316a_default-eSc_textblock_0acb96cb" type="Paratext">
               <l corresp="#f_74_5316a_default-eSc_line_cee73c21">ידר זעים</l>
            </ab>
            <milestone xml:id="f_74_5316a_default.jpg-Margin" unit="Margin"/>
            <ab corresp="#f_74_5316a_default-eSc_textblock_518b0aaa" type="Margin">
               <l corresp="#f_74_5316a_default-eSc_line_793e81b6">לנד</l>
            </ab>
            <milestone xml:id="f_74_5316a_default.jpg-fw" unit="fw"/>
            <ab corresp="#f_74_5316a_default-eSc_dummyblock_" type="-fw">
               <l corresp="#f_74_5316a_default-eSc_line_4efdce57">זאת התגרה סיאנתיה-</l>
               <l corresp="#f_74_5316a_default-eSc_line_9dcb4285">להפך בטעות מה שלאור</l>
               <l corresp="#f_74_5316a_default-eSc_line_850f7369">צדיך לסמן שלמע"ל ומה ש</l>
               <l corresp="#f_74_5316a_default-eSc_line_879c531a">מעלה למטה ולכן נן ינן</l>
               <l corresp="#f_74_5316a_default-eSc_line_cedbd99d">נידן</l>
            </ab>
