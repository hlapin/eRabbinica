            <ab corresp="#r_396_19ac2_default-eSc_textblock_a88ef516" type="Commentary">
               <l corresp="#l_396_19ac2_default-eSc_line_bd714590"/>
               <l corresp="#l_396_19ac2_default-eSc_line_8a2a8988"/>
               <l corresp="#l_396_19ac2_default-eSc_line_2f4942cc"/>
               <l corresp="#l_396_19ac2_default-eSc_line_730f91cb"/>
               <l corresp="#l_396_19ac2_default-eSc_line_0efa8c38"/>
