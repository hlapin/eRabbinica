            <pb xml:id="p_195_ae31b_default.jpg"/>
            <cb xml:id="c_195_ae31b_default.jpg-A"/>
            <ab corresp="#r_195_ae31b_default-eSc_textblock_9f6c5170" type="Main">
               <l corresp="#l_195_ae31b_default-eSc_line_83b6673b">ה'</l>
               <l corresp="#l_195_ae31b_default-eSc_line_d6fdc1b0">התחתונה חמש רובד ניש ׆'</l>
               <l corresp="#l_195_ae31b_default-eSc_line_d48ac477">האמינעית שש והרובד שבע העליונה</l>
               <l corresp="#l_195_ae31b_default-eSc_line_efc01e66">שבע שנ' הייציע התחתונה חמש באמה</l>
            </ab>
            <cb xml:id="c_195_ae31b_default.jpg-B"/>
            <ab corresp="#r_195_ae31b_default-eSc_textblock_308ad6d5" type="Main">
               <l corresp="#l_195_ae31b_default-eSc_line_a0683f7b">רחכה וגומר:</l>
            </ab>
            <ab corresp="#r_195_ae31b_default-eSc_textblock_1fadd6dd" type="Main">
               <l corresp="#l_195_ae31b_default-eSc_line_c8f5972c">ז'</l>
               <l corresp="#l_195_ae31b_default-eSc_line_ca536b82">ומסיבה היתה</l>
               <l corresp="#l_195_ae31b_default-eSc_line_c0a01b70">עולה מקרן</l>
               <l corresp="#l_195_ae31b_default-eSc_line_66675184">מזרחית דרומי'</l>
               <l corresp="#l_195_ae31b_default-eSc_line_7ec3591f">לקרן צפונית</l>
               <l corresp="#l_195_ae31b_default-eSc_line_ea3287c1">מערבות שבהן</l>
               <l corresp="#l_195_ae31b_default-eSc_line_f829b631">היועולין לגגות</l>
               <l corresp="#l_195_ae31b_default-eSc_line_18a09e7d">התאין היה עולה במסיבה ופש למערב</l>
               <l corresp="#l_195_ae31b_default-eSc_line_6b550949">הילך את כל הצפון עד שהו מגיע</l>
               <l corresp="#l_195_ae31b_default-eSc_line_4bdc9553">למערב הגיע למערב ופך פנו לדרוס</l>
               <l corresp="#l_195_ae31b_default-eSc_line_76b06394">הילך את כל המערב עד שהוא מגיע</l>
               <l corresp="#l_195_ae31b_default-eSc_line_2516f0a1">לדרוס הגע לדרוס והפך פניו למזרח</l>
               <l corresp="#l_195_ae31b_default-eSc_line_6698035b">יה מהלך בדרוס עד שהוא מגיע ל-</l>
               <l corresp="#l_195_ae31b_default-eSc_line_853e2e0d">פתחה שלעלייה שפתחה שלעלייה</l>
               <l corresp="#l_195_ae31b_default-eSc_line_aa209d3e">פתוח לדרוס:</l>
               <l corresp="#l_195_ae31b_default-eSc_line_fa8a9ea1">ז'</l>
               <l corresp="#l_195_ae31b_default-eSc_line_20c53e43">ובפתחה שלעלייה היו ׆'</l>
               <l corresp="#l_195_ae31b_default-eSc_line_5eeae708">שם שני כלונסות שלארז שבהן היו</l>
               <l corresp="#l_195_ae31b_default-eSc_line_f7b8aa60">עולין לגגה שלעלייה ובראשן פספסין</l>
               <l corresp="#l_195_ae31b_default-eSc_line_a615f4a6">מבדיל בעלייה בין קדש לבין קדש ה-</l>
               <l corresp="#l_195_ae31b_default-eSc_line_90a70c11">קדשים ובלולים היו פתוחין בעלייה ל-</l>
               <l corresp="#l_195_ae31b_default-eSc_line_f67cdb54">בית קדש הקדשים שבהן משלשלים</l>
               <l corresp="#l_195_ae31b_default-eSc_line_c32928b5">את האומנים בתיבות כדי שלא יזונו</l>
               <l corresp="#l_195_ae31b_default-eSc_line_18de2172">את עציהם מבית קדש הקדשים:</l>
            </ab>
