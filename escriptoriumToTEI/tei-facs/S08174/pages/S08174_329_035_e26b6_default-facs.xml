   <text>
      <body>
         <div>
            <pb xml:id="p_329_035_e26b6_default"/>
            <cb xml:id="c_329_035_e26b6_default-A"/>
            <ab corresp="#r_35_e26b6_default-eSc_textblock_cd5fd049" type="Paratext">
               <l corresp="#l_35_e26b6_default-eSc_line_1815584b">מסכת זכחים</l>
            </ab>
            <ab corresp="#r_35_e26b6_default-eSc_textblock_5b0af523" type="Main">
               <l corresp="#l_35_e26b6_default-eSc_line_0f995528">אומ' עד שיכפר. ר' יהודה אומ אם הכנים</l>
               <l corresp="#l_35_e26b6_default-eSc_line_94b5bf5c">שוגג כשר. כל הדמים הפסולין שניתנו</l>
               <l corresp="#l_35_e26b6_default-eSc_line_18d3cf53">על גבי המזכח לא הרצה הציץ אלא על</l>
               <l corresp="#l_35_e26b6_default-eSc_line_8c376621">הטמא שהציץ מרצה על הטמא ואינו</l>
               <l corresp="#l_35_e26b6_default-eSc_line_e3e3d33e">מרצה  על  היוצא: <milestone unit="div3" n="5.1.9"/>
               </l>
            </ab>
            <ab corresp="#r_35_e26b6_default-eSc_textblock_66beec47" type="Commentary">
               <l corresp="#l_35_e26b6_default-eSc_line_bdfe156a"/>
               <l corresp="#l_35_e26b6_default-eSc_line_751d9da4"/>
               <l corresp="#l_35_e26b6_default-eSc_line_44a04fa7"/>
               <l corresp="#l_35_e26b6_default-eSc_line_3e9d3c3a"/>
               <l corresp="#l_35_e26b6_default-eSc_line_77719c4f"/>
               <l corresp="#l_35_e26b6_default-eSc_line_83080646"/>
               <l corresp="#l_35_e26b6_default-eSc_line_94e00325"/>
