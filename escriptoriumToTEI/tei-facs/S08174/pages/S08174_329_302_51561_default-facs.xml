   <text>
      <body>
         <div>
            <pb xml:id="p_329_302_51561_default"/>
            <cb xml:id="c_329_302_51561_default-A"/>
            <ab corresp="#r_302_51561_default-eSc_textblock_16302cd8" type="Paratext">
               <l corresp="#l_302_51561_default-eSc_line_225b4404">סדר טפרות</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_ed38d3ed" type="Main">
               <l corresp="#l_302_51561_default-eSc_line_5d9f8bae">מטמאין ובית הלל מטהרין שמאי או'</l>
               <l corresp="#l_302_51561_default-eSc_line_cb4573e8">אף מלבן שלכסא טמא. כסא שקבעו</l>
               <l corresp="#l_302_51561_default-eSc_line_df2ba54d">בעריבה בית שמאי מטמאין ובית הלל</l>
               <l corresp="#l_302_51561_default-eSc_line_e7120a19">מטהרין שמאי אומ' אף העשוי בה:</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_8bb19d57" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_891c158f"/>
               <l corresp="#l_302_51561_default-eSc_line_c6d0e4a3"/>
               <l corresp="#l_302_51561_default-eSc_line_1cfc48e3"/>
               <l corresp="#l_302_51561_default-eSc_line_870521e1"/>
               <l corresp="#l_302_51561_default-eSc_line_66ad8242"/>
               <l corresp="#l_302_51561_default-eSc_line_75beffce"/>
               <l corresp="#l_302_51561_default-eSc_line_58d74880"/>
               <l corresp="#l_302_51561_default-eSc_line_7f25dd64"/>
               <l corresp="#l_302_51561_default-eSc_line_7eb5a19f"/>
               <l corresp="#l_302_51561_default-eSc_line_3210d198"/>
               <l corresp="#l_302_51561_default-eSc_line_f97aceaa"/>
               <l corresp="#l_302_51561_default-eSc_line_c7b55cf6"/>
               <l corresp="#l_302_51561_default-eSc_line_d40afe00"/>
               <l corresp="#l_302_51561_default-eSc_line_872b73b0"/>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_064d9044" type="Main">
               <l corresp="#l_302_51561_default-eSc_line_3d2bf790">ה'</l>
               <l corresp="#l_302_51561_default-eSc_line_6969b027">
                  <milestone unit="ab" n="6.1.22.5"/>כסא  שלא  היו  חיפוייו   יוצאין  ניטלו  </l>
               <l corresp="#l_302_51561_default-eSc_line_eeecb428">טמא שכן דרכו להיות מטהו על צרו</l>
               <l corresp="#l_302_51561_default-eSc_line_6baa8e31">ויושב עליו:</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_7ee1437a" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_1233b182"/>
               <l corresp="#l_302_51561_default-eSc_line_4a12657b"/>
               <l corresp="#l_302_51561_default-eSc_line_b69a6b25"/>
               <l corresp="#l_302_51561_default-eSc_line_fb858bcb"/>
               <l corresp="#l_302_51561_default-eSc_line_a2b9b7b6"/>
               <l corresp="#l_302_51561_default-eSc_line_51669800"/>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_8849a19f" type="Main">
               <l corresp="#l_302_51561_default-eSc_line_0a611c5a">ז'</l>
               <l corresp="#l_302_51561_default-eSc_line_a6dc8a48">
                  <milestone unit="ab" n="6.1.22.6"/>כסוא  </l>
               <l corresp="#l_302_51561_default-eSc_line_d3a319c5">שניטל חיפוין האמצעי והחיצונים</l>
               <l corresp="#l_302_51561_default-eSc_line_26730000">קיימים טמא. ניטלו החיצונים וה-</l>
               <l corresp="#l_302_51561_default-eSc_line_9e024403">אמצעי קיים טמא. ר' שמעון אומר</l>
               <l corresp="#l_302_51561_default-eSc_line_8ddc9bbb">אם היה רחב טפח:</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_99259447" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_6abd6471"/>
               <l corresp="#l_302_51561_default-eSc_line_66780654"/>
               <l corresp="#l_302_51561_default-eSc_line_5de4c21c"/>
               <l corresp="#l_302_51561_default-eSc_line_2469e964"/>
               <l corresp="#l_302_51561_default-eSc_line_ba81ea7f"/>
               <l corresp="#l_302_51561_default-eSc_line_579b2125"/>
            </ab>
            <cb xml:id="c_329_302_51561_default-B"/>
            <ab corresp="#r_302_51561_default-eSc_textblock_fb59974f" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_dfedf402"/>
               <l corresp="#l_302_51561_default-eSc_line_77194e76"/>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_113fa527" type="Main">
               <l corresp="#l_302_51561_default-eSc_line_968166e8">ז'</l>
               <l corresp="#l_302_51561_default-eSc_line_60dd394e">
                  <milestone unit="ab" n="6.1.22.7"/>כסא </l>
               <l corresp="#l_302_51561_default-eSc_line_3153dcc3">שניטלו שנים מחיפוייו זה בצד זה ר'</l>
               <l corresp="#l_302_51561_default-eSc_line_4a887415">עקיבה מטמא וחכמים מטהרין. אמ'</l>
               <l corresp="#l_302_51561_default-eSc_line_3391e21e">ר' יהודה אף כסא שלכלה שניטל וּוּ׆'</l>
               <l corresp="#l_302_51561_default-eSc_line_2450dea3">חיפווו נשתייר כו בית קכלה טהור</l>
               <l corresp="#l_302_51561_default-eSc_line_64018e76">מפני שכטל העיקר וכטילה הטפילה:</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_8db5fae6" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_591dca9d"/>
               <l corresp="#l_302_51561_default-eSc_line_f212c10e"/>
               <l corresp="#l_302_51561_default-eSc_line_97d3b681"/>
               <l corresp="#l_302_51561_default-eSc_line_d8163e59"/>
               <l corresp="#l_302_51561_default-eSc_line_322a197a"/>
               <l corresp="#l_302_51561_default-eSc_line_3e462503"/>
               <l corresp="#l_302_51561_default-eSc_line_b28c5aaf"/>
               <l corresp="#l_302_51561_default-eSc_line_9b28e804"/>
               <l corresp="#l_302_51561_default-eSc_line_51093b12"/>
               <l corresp="#l_302_51561_default-eSc_line_06112839"/>
               <l corresp="#l_302_51561_default-eSc_line_1665b372"/>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_7a971a7e" type="Main">
               <l corresp="#l_302_51561_default-eSc_line_68cffa87">ח'</l>
               <l corresp="#l_302_51561_default-eSc_line_f99d8ba5">
                  <milestone unit="ab" n="6.1.22.8"/>שידה  שניטלה   העליון  טמא'‏  </l>
               <l corresp="#l_302_51561_default-eSc_line_5e6eecda">מפני התחתון ניטל התחתון טמאה</l>
               <l corresp="#l_302_51561_default-eSc_line_7e0f1aaa">מפני העליון ניטל העליון והתחתון ר'</l>
               <l corresp="#l_302_51561_default-eSc_line_8c1ed01a">יהודה מטמא מפני הדפין וחכמים</l>
               <l corresp="#l_302_51561_default-eSc_line_c6dec9f7">מטהרין. ישיבת הסתת טמאה:</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_0b72f3e9" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_fb4916ad"/>
               <l corresp="#l_302_51561_default-eSc_line_ef06899d"/>
               <l corresp="#l_302_51561_default-eSc_line_696d91f6"/>
               <l corresp="#l_302_51561_default-eSc_line_0083642d"/>
               <l corresp="#l_302_51561_default-eSc_line_d286748a"/>
               <l corresp="#l_302_51561_default-eSc_line_84389d51"/>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_7ab71ce7" type="Main">
               <l corresp="#l_302_51561_default-eSc_line_dd909af7">ט'</l>
               <l corresp="#l_302_51561_default-eSc_line_6d9ba2e1">
                  <milestone unit="ab" n="6.1.22.9"/>כופת  שסרקו   וכרכמו  ועשאו  פנים  ר'‏  </l>
               <l corresp="#l_302_51561_default-eSc_line_643ed8e1">עקיבה מטמא וחכמים מטהרין עד</l>
               <l corresp="#l_302_51561_default-eSc_line_b9e16403">שיחוק בו. הסל והכלכלה שמלאן תנן</l>
               <l corresp="#l_302_51561_default-eSc_line_39b1cf37">או מוכין והתקינן לישיבה טהורין. ׆'</l>
               <l corresp="#l_302_51561_default-eSc_line_4247b280">סרגן בגמי או במשיחה טמאין:</l>
            </ab>
            <ab corresp="#r_302_51561_default-eSc_textblock_b262152e" type="Commentary">
               <l corresp="#l_302_51561_default-eSc_line_18a8f2a7"/>
               <l corresp="#l_302_51561_default-eSc_line_f3f50319"/>
            </ab>
            <milestone unit="pageSection" type="notInMain"/>
         </div>
