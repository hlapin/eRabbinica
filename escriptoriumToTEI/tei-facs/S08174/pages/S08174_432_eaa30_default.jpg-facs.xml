            <ab corresp="#r_432_eaa30_default-eSc_textblock_d4f8b345" type="Main">
               <l corresp="#l_432_eaa30_default-eSc_line_10235731"><!-- 06.05 -->מסכת טהרת</l>
            </ab>
            <ab corresp="#r_432_eaa30_default-eSc_textblock_fdd79104" type="Main">
               <l corresp="#l_432_eaa30_default-eSc_line_b9a94c37">שלשה</l>
               <l corresp="#l_432_eaa30_default-eSc_line_a1d92d46">עשר דבר בנכלת ה-</l>
               <l corresp="#l_432_eaa30_default-eSc_line_510d895b">עוף הטהור צריכה</l>
               <l corresp="#l_432_eaa30_default-eSc_line_aeffda91">מחשבה ואינה צריכה</l>
               <l corresp="#l_432_eaa30_default-eSc_line_64d22644">הכשר ומטמא טומאת</l>
               <l corresp="#l_432_eaa30_default-eSc_line_d94b21e2">אוכלין כביצה וכזית בבית הבליעה. וה</l>
               <l corresp="#l_432_eaa30_default-eSc_line_536bafad">אוכלה טעון הערב שמש וחייבין על וּ׆'</l>
               <l corresp="#l_432_eaa30_default-eSc_line_ee25cb2d">ביאת המקדש ושורפין עליה את התרומה</l>
               <l corresp="#l_432_eaa30_default-eSc_line_55855531">והאוכל אבר מן החי ממנה סופג את ה-</l>
               <l corresp="#l_432_eaa30_default-eSc_line_f76c13e0">ארבעים שחיטתה ומליקתה מטהרות</l>
               <l corresp="#l_432_eaa30_default-eSc_line_2d27b818">טריפתה דברי ר'‏ מאיר ר' יהודה אומ' אינן</l>
               <l corresp="#l_432_eaa30_default-eSc_line_5ba0e299">מטהרות. ר' יוסי אומ' שחיטתה מטהרת</l>
               <l corresp="#l_432_eaa30_default-eSc_line_7cbec652">אכל לא מליקתה:</l>
            </ab>
            <cb xml:id="c_432_eaa30_default.jpg-B"/>
            <ab corresp="#r_432_eaa30_default-eSc_textblock_dc1262cc" type="Paratext">
               <l corresp="#l_432_eaa30_default-eSc_line_f6e0410b">דר טהרות</l>
            </ab>
