            <ab corresp="#r_387_a1ff4_default-eSc_textblock_546c1037" type="Paratext">
               <l corresp="#l_387_a1ff4_default-eSc_line_ab6fafbe">מסכ' כתובות</l>
            </ab>
            <ab corresp="#r_387_a1ff4_default-eSc_textblock_ae685116" type="Commentary">
               <l corresp="#l_387_a1ff4_default-eSc_line_b70c0555"/>
