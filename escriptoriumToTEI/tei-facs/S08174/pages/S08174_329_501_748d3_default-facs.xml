            <ab corresp="#r_501_748d3_default-eSc_textblock_c93f6049" type="Paratext">
               <l corresp="#l_501_748d3_default-eSc_line_6b4322b5">מסכת נידה</l>
            </ab>
            <ab corresp="#r_501_748d3_default-eSc_textblock_15e1150e" type="Commentary">
               <l corresp="#l_501_748d3_default-eSc_line_134a1e18"/>
