            <ab corresp="#r_539_a9ce4_default-eSc_textblock_3c4f7f28" type="Main">
               <l corresp="#l_539_a9ce4_default-eSc_line_d4cba8f2">ב'</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_963f4871">
                  <milestone unit="ab" n="6.11.1.2"/>ככל   הכלים  נותנין  לידים  אפילו  בכלי </l>
               <l corresp="#l_539_a9ce4_default-eSc_line_6d815caa">גללים וככלי אבנים ובכלי אדמה ואין</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_78fdbfa5">נותנין לידים לא בדפנות הכלים ולא</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_c1a71f1b">בשולי המחץ ולא במגופת החכית</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_a910ad93">ולא יתן לחבירו בחפניו מפני שאין</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_3c13b363">ממלאין ואין מקדשין ואין מזין מי</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_e2822e7a">חטאת ואין נותנין לידים אלא בכלי</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_dc1b9bb0">ואין מצילין בצמיד פתיל אלא כלים</l>
               <l corresp="#l_539_a9ce4_default-eSc_line_d859bed3">שאין מצילין כלי חרש אלא כלים:</l>
            </ab>
            <ab corresp="#r_539_a9ce4_default-eSc_textblock_1fd5bd12" type="Commentary">
               <l corresp="#l_539_a9ce4_default-eSc_line_2aad4769"/>
               <l corresp="#l_539_a9ce4_default-eSc_line_9fd6ac23"/>
               <l corresp="#l_539_a9ce4_default-eSc_line_658ff92e"/>
