               <l corresp="#l_QafahZeraimMaimArabic-eSc_line_5477347b">פאה פרק ששי</l>
               <l corresp="#l_QafahZeraimMaimArabic-eSc_line_7029231b">בית י</l>
               <l corresp="#l_QafahZeraimMaimArabic-eSc_line_b489817c"/>
