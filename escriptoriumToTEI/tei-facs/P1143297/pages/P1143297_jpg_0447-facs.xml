            <ab corresp="#r_jpg_0447-eSc_textblock_8e466bc0" type="Commentary">
               <l corresp="#l_jpg_0447-eSc_line_ea07f9c5"/>
               <l corresp="#l_jpg_0447-eSc_line_d33bf6ba"/>
               <l corresp="#l_jpg_0447-eSc_line_60e67517"/>
               <l corresp="#l_jpg_0447-eSc_line_d8c1ba79"/>
