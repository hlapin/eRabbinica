            <ab corresp="#jpg_0534-eSc_textblock_40943525" type="Margin">
               <l corresp="#jpg_0534-eSc_line_ecc21482">בהעלותך</l>
            </ab>
            <milestone xml:id="jpg_0534.jpg-fw" unit="fw"/>
