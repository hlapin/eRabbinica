               <l corresp="#l_QafahNeziqinMaimArabic-eSc_line_54c49016">גדיות</l>
               <l corresp="#l_QafahNeziqinMaimArabic-eSc_line_c17e183f">פרק שני</l>
               <l corresp="#l_QafahNeziqinMaimArabic-eSc_line_6030763e"/>
